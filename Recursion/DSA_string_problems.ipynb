      "source": [
        "def permute(arr,left,right):\n",
        "  # Base condition\n",
        "  if left==right:\n",
        "    print(\"\".join(arr))\n",
        "    return\n",
        "\n",
        "  else:\n",
        "    for i in range(left,right+1):\n",
        "      arr[left],arr[i]=arr[i],arr[left]\n",
        "      permute(arr,left+1,right)\n",
        "      arr[left],arr[i]=arr[i],arr[left]\n",
        "\n",
        "permute(list(\"ABC\"),0,2)"
      ]
    }
  ]
